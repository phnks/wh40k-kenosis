    <profile id="5ee3ffd9-73ef-488f-9f78-3a7f28c1e170" name="Firepoint Telemetry Cache" hidden="false" typeId="0b226e7f-db6d-461f-990c-5501b9ccc9e8" typeName="Abilities">
