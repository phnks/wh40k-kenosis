    <profile id="252d6e87-727d-4296-9f77-4136fdcdf1b3" name="Mag-inverter Shield" publicationId="4a469c26-9989-4da4-8c19-71e7955f9727" hidden="false" typeId="0b226e7f-db6d-461f-990c-5501b9ccc9e8" typeName="Abilities">
